{
    "name": "Sackitey Adblocker",
    "version": "1.0",
    "description": "To block unwanted ads",
    "permissions": [
        "declarativeNetRequest"
    ],


    "icons": {
        "16": "icons/logo_16x16.png",
        "48": "icons/logo_48x48.png",
        "128": "icons/logo_128x128.png"
    },
    "host_permissions": [
        "*://*/*"
    ],
    "declarative_net_request": {
        "rule_resources": [
            {
                "id": "ruleset_1",
                "enabled": true,
                "path": "rules.json"
            }
        ]
    },
    "manifest_version": 3
}
